{
    "manifest_version": 3,
    "name": "SecondBrain Extension",
    "version": "1.0",
    "description": "Chrome extension for SecondBrain app",
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "host_permissions": [
        "http://localhost:5001/*"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}